<odoo>
	<record model="ir.ui.view" id="invoicing_view_form">
			<field name="name">invoicing.view.form</field>
			<field name="model">account.move</field>
			<field name="inherit_id" ref="account.view_move_form"/>
			<field name="arch" type="xml">
				<xpath expr="//field[@name='delivery_date']" position="after">
					<field name="delivery_info"/>
					<field name="delivery_status"/>
				</xpath>
			</field>
	</record>
	<record id="view_move_form" model="ir.ui.view">
            <field name="name">account.move.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
				<xpath expr="//field[@name ='amount_residual']" position="before">
					<field name="penalty_amount"/>
				</xpath>
			</field>
	</record>

