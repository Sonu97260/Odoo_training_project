<odoo>
  <data>
    <record id="cron_active_jobs" model="ir.cron">
        <field name="name">Auto active Jobs</field>
        <field name="model_id" ref="model_job_position"/>
        <field name="state">code</field>
        <field name="code">model.active_jobs()</field>
        <field name="user_id" ref="base.user_admin"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="active" eval="True"/>        
      </record>
    </data>
</odoo>